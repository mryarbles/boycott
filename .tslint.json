{
	"rules": {
		"class-name": true,
		"comment-format": [
			false,
			"check-space"
		],
		"indent": [
			true,
			"tab"
		],
		"no-duplicate-variable": true,
		"no-eval": true,
		"no-internal-module": true,
		"no-trailing-whitespace": false,
		"no-var-keyword": false,
		"one-line": [
			true,
			"check-open-brace",
			"check-whitespace"
		],
		"quotemark": [
			false,
			"single"
		],
		"semicolon": [
			true,
			"always"
		],
		"triple-equals": [
			true,
			"allow-null-check"
		],
		"variable-name": [
			true,
			"ban-keywords"
		]
	}
}